{
  "name": "stato-pronto-soccorso",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:test": "prettier --list-different  \"./projects/**/src/**/*.{ts,html,css,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/src/**/*.{ts,html,css,scss,json}\"",
    "lint": "ng lint",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template sunburst --metadata dist/stato-ps/stats.json --filename dist/stato-ps/analyze/index.html && http-server -o -c-1 ./dist/stato-ps/analyze/",
    "analyze:sme": "ng build --stats-json --output-hashing none --named-chunks && source-map-explorer dist/stato-ps/browser/*.js --html dist/stato-ps/sme/index.html && http-server -o -c-1 ./dist/stato-ps/sme/"


  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "~5.1.0",
    "esbuild-visualizer": "^0.6.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.3.2"
  }
}
